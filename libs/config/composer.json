{
    "name": "helix/config",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "symfony/yaml": "^5.4|^6.0",
        "helix/arrayable-contracts": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Helix\\Config\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "vimeo/psalm": "^4.22"
    },
    "autoload-dev": {
        "psr-4": {
            "Helix\\Config\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
