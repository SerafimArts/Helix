{
    "name": "helix/config",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "helix/arrayable-contracts": "^1.0",
        "justinrainbow/json-schema": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Helix\\Config\\": "src"
        }
    },
    "require-dev": {
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
