{
    "name": "helix/server",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "psr/http-server-middleware": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "helix/http": "^1.0",
        "helix/container-contracts": "^1.0",
        "helix/http-status-code": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Helix\\Server\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20"
    },
    "autoload-dev": {
        "psr-4": {
            "Helix\\Server\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
