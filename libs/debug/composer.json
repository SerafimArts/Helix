{
    "name": "helix/debug",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Helix\\Debug\\": "src"
        }
    },
    "require-dev": {
        "jetbrains/phpstorm-attributes": "^1.0",
        "helix/foundation-console": "^1.0",
        "phpunit/phpunit": "^9.5.20",
        "vimeo/psalm": "^4.22"
    },
    "autoload-dev": {
        "psr-4": {
            "Helix\\Debug\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
