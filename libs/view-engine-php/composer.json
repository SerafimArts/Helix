{
    "name": "helix/view-engine-php",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "helix/view": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Helix\\View\\Engine\\Php\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "vimeo/psalm": "^4.22"
    },
    "autoload-dev": {
        "psr-4": {
            "Helix\\View\\Engine\\Php\\Tests\\": "tests"
        }
    },
    "provide": {
        "helix/view-contracts-implementation": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
