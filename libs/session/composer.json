{
    "name": "helix/session",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-session": "*",
        "ramsey/uuid": "^4.3",
        "helix/clock": "^1.0",
        "helix/memoizable-contracts": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Helix\\Session\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "vimeo/psalm": "^4.22"
    },
    "autoload-dev": {
        "psr-4": {
            "Helix\\Session\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
