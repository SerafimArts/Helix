{
    "name": "helix/database-pdo",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-pdo": "*",
        "helix/database": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Helix\\Database\\PDO\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Helix\\Database\\PDO\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
