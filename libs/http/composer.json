{
    "name": "helix/http",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "nyholm/psr7": "^1.4",
        "helix/http-method": "^1.0",
        "helix/http-status-code": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Helix\\Http\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20"
    },
    "autoload-dev": {
        "psr-4": {
            "Helix\\Http\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
