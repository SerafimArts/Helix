{
    "name": "helix/view",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "helix/view-contracts": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Helix\\View\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20"
    },
    "autoload-dev": {
        "psr-4": {
            "Helix\\View\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
