{
    "name": "helix/param-resolver",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "psr/container": "^1.0|^2.0"
    },
    "autoload": {
        "psr-4": {
            "Helix\\ParamResolver\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20"
    },
    "autoload-dev": {
        "psr-4": {
            "Helix\\ParamResolver\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
