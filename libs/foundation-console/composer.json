{
    "name": "helix/foundation-console",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "helix/foundation": "^1.0",
        "symfony/console": "^5.4|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Helix\\Foundation\\Console\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "vimeo/psalm": "^4.22"
    },
    "autoload-dev": {
        "psr-4": {
            "Helix\\Foundation\\Console\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
