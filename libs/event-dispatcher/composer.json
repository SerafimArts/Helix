{
    "name": "helix/dispatcher",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.1",
        "ramsey/uuid": "^4.3",
        "helix/event-dispatcher-contracts": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Helix\\Dispatcher\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "vimeo/psalm": "^4.22"
    },
    "autoload-dev": {
        "psr-4": {
            "Helix\\Dispatcher\\Tests\\": "tests"
        }
    },
    "provide": {
        "helix/event-dispatcher-contracts": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
