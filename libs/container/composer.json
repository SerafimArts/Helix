{
    "name": "helix/container",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "psr/container": "^2.0",
        "helix/memoizable-contracts": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Helix\\Container\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20"
    },
    "autoload-dev": {
        "psr-4": {
            "Helix\\Container\\Tests\\": "tests"
        }
    },
    "provide": {
        "helix/container-contracts-implementation": "^1.0",
        "helix/dispatcher-contracts-implementation": "^1.0",
        "helix/container-registrar-contracts-implementation": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
