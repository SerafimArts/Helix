{
    "name": "helix/foundation",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "helix/boot": "^1.0",
        "helix/config": "^1.0",
        "helix/container": "^1.0",
        "helix/error-handler-contracts": "^1.0",
        "psr/log": "^1.0|^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Helix\\Foundation\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "vimeo/psalm": "^4.22"
    },
    "autoload-dev": {
        "psr-4": {
            "Helix\\Foundation\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
