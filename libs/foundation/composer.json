{
    "name": "helix/foundation",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "helix/config": "^1.0",
        "helix/error-handler-contracts": "^1.0",
        "psr/log": "^1.0|^2.0|^3.0",
        "helix/container": "^1.0",
        "helix/boot": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Helix\\Foundation\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
