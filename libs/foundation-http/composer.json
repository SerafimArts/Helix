{
    "name": "helix/foundation-http",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "helix/foundation": "^1.0",
        "helix/async": "^1.0",
        "helix/error-handler-http-contracts": "^1.0",
        "helix/server-implementation": "^1.0",
        "helix/http": "^1.0",
        "helix/router": "^1.0",
        "helix/view": "^1.0",
        "helix/middleware": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message-implementation": "^1.0",
        "psr/http-factory-implementation": "^1.0",
        "filp/whoops": "^2.14"
    },
    "autoload": {
        "psr-4": {
            "Helix\\Foundation\\Http\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
